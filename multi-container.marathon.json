{
    "id": "/dnowell-multi-container",
    "groups": [
	{
	"id": "/dnowell-multi-container/web",
    "instances": 2,
    "cpus": 0.25,
    "mem": 32,

    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "docker-pilot.dsc.umich.edu:31111/multi-container-web:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "healthChecks": [
        {
            "protocol": "HTTP"
        }
    ],
    "backoffSeconds": 2,
    "backoffFactor": 1.25,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.5
    },
  },
    {
    "id": "/dnowell-multi-container/db",
    "instances": 2,
    "cpus": 0.25,
    "mem": 32,

    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "docker-pilot.dsc.umich.edu:31111/multi-container-db:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 27017,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "backoffSeconds": 2,
    "backoffFactor": 1.25,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.5
    }
  }
]

}
